{
  "kind": "collectionType",
  "collectionName": "tech_stacks",
  "info": {
    "singularName": "tech-stack",
    "pluralName": "tech-stacks",
    "displayName": "Portfolio/TechStack",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true,
      "default": "No description."
    },
    "website": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "sortIndex": {
      "type": "integer",
      "required": true,
      "default": 1,
      "min": 0
    },
    "tech_stack_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tech-stack-tag.tech-stack-tag",
      "inversedBy": "portfolio_tech_stacks"
    }
  }
}
